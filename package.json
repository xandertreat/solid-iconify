{
	"author": "Xander Treat",
	"name": "@xtreat/solid-iconify",
	"version": "0.0.1",
	"description": "A reactive and first class native Solid Iconify icon component",
	"license": "MIT",
	"keywords": [
		"solid.js",
		"solid",
		"iconify",
		"icon",
		"svg",
		"compiled",
		"bundled",
		"typescript",
		"javascript",
		"ts",
		"js",
		"cjs",
		"mjs"
	],
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"sideEffects": false,
	"homepage": "https://github.com/xandertreat/solid-iconify#readme",
	"bugs": {
		"url": "https://github.com/xandertreat/solid-iconify/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xandertreat/solid-iconify.git"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@iconify/types": "^2.0.0",
		"@solidjs/router": ">=0.15.0",
		"@solidjs/testing-library": "^0.8.10",
		"@types/node": "^22.15.30",
		"@vitest/browser": "^3.2.2",
		"@vitest/coverage-v8": "3.1.4",
		"playwright": "^1.52.0",
		"solid-js": ">=1.8.0",
		"tsdown": "^0.12.7",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"unplugin-unused": "^0.5.0",
		"vite-plugin-solid": "^2.11.6",
		"vitest": "^3.2.2"
	},
	"peerDependencies": {
		"solid-js": ">=1.8.0",
		"@solidjs/router": ">=0.15.0"
	},
	"dependencies": {
		"@thednp/domparser": "^0.1.2",
		"xss": "^1.0.15"
	},
	"trustedDependencies": ["@biomejs/biome"],
	"scripts": {
		"build": "bun tsdown",
		"test": "vitest test",
		"ci": "bun biome ci; bun tsc --noEmit; bun test; bun run build;",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage",
		"test:browser": "vitest",
		"check": "biome check; bun tsc",
		"check:fix": "biome check --write; bun tsc",
		"format": "biome format --write",
		"format:check": "biome format",
		"lint": "bun check",
		"lint:fix": "bun check:fix"
	}
}
