	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"test": "vitest test",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage",
		"test:browser": "vitest",
		"check": "biome check; bun typecheck",
		"check:fix": "biome check --write; bun typecheck",
		"format": "biome format --write",
		"format:check": "biome format",
		"typecheck": "tsc --noEmit",
		"lint": "bun check",
		"lint:fix": "bun check:fix",
		"pkg:changeset": "changeset",
		"pkg:version": "changeset version",
		"pkg:publish": "bun run build && changeset publish",
		"ci": "bun biome ci; bun typecheck; bun run build; bun test;"
	},
	"browser": {
		"./dist/server.js": "./dist/index.js",
		"./dist/server.cjs": "./dist/index.cjs"
