		"node": ">=20.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@changesets/cli": "^2.29.4",
		"@iconify/types": "^2.0.0",
		"@solidjs/testing-library": "^0.8.10",
		"@types/node": "^22.15.30",
		"@vitest/browser": "^3.2.2",
		"@vitest/coverage-v8": "3.1.4",
		"playwright": "^1.52.0",
		"solid-js": ">=1.9.0",
		"tslib": "^2.8.1",
