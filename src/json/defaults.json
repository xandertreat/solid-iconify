{
	"ICONIFY_API": "api.iconify.design",
	"REQUEST_OPTIONS": {
		"cache": "force-cache"
	},
	"COLLECTION_LIMIT": 32,
	"ICON_LIMIT": 64,
	"DEFAULT_SVG_ATTRIBUTES": {
		"xmlns": "http://www.w3.org/2000/svg",
		"width": "1em",
		"height": "1em",
		"viewBox": "0 0 16 16",
		"fill": "currentColor",
		"role": "img",
		"style": {
			"aspect-ratio": "1/1",
			"background-color": "transparent",
			"display": "inline-block"
		}
	},
	"SVG_TITLE": false,
	"SHOW_LOADING_DEFAULT": false,
	"SHOW_ERROR_DEFAULT": true,
	"SANITIZE": true,
	"SANITIZE_OPTIONS": {}
}
